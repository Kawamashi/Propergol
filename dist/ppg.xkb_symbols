    key <AB09> {[ w               , W               , bar             , brokenbar       , underscore      , VoidSymbol      ]}; // w W | ¦ _
    key <AB10> {[ ISO_Level5_Latch, semicolon       , asciitilde      , dead_tilde      , VoidSymbol      , VoidSymbol      ]}; // ' ; ~ ~

    // Pinky keys
