    key <AD07> {[ m               , M               , quotedbl        , dead_doubleacute, guillemotleft   , U201C           ]}; // m M " ˝ « “
    key <AD08> {[ c               , C               , equal           , notequal        , guillemotright  , U201D           ]}; // c C = ≠ » ”
    key <AD09> {[ U2019           , exclam          , exclam          , notsign         , U2022           , exclamdown      ]}; // ’ ! ! ¬ • ¡
    key <AD10> {[ x               , X               , grave           , dead_grave      , periodcentered  , VoidSymbol      ]}; // x X ` ` ·

    // Letters, second row
    key <AC01> {[ o               , O               , question        , dead_caron      , ecircumflex     , Ecircumflex     ]}; // o O ? ˇ ê Ê
